{
    "manifest_version": 2,
    "content_scripts": [ {
        "exclude_globs":    [  ],
        "include_globs":    [ "*" ],
        "js":               [ "twitterForceMobile.user.js" ],
        "matches":          [
          "https://twitter.com/*",
          "https://www.twitter.com/*",
          "http://www.twitter.com/*",
          "http://twitter.com/*"
        ],
        "run_at": "document_start"
    } ],
    "converted_from_user_script": true,
    "description":  "Forwards you to the mobile version of twitter",
    "name":         "Twitter Force Mobile",
    "version":      "1.0.2",
    "icons": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
}
